#this part of the script iterates a for loop through example.txt
for n in $(cat example.txt)
do 
echo "this next variable is $n"
done


#this part of the script searches for poly A in fasta file
echo "searching for poly As in SARS COV2 fasta file"
grep 'AAAA' NC_045512.fa>polyA
echo "search is complete"